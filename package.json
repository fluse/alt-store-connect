        "babel-preset-es2015": "^6.6.0",
        "babel-preset-react": "^6.5.0",
        "chai": "3.3.0",
        "immutable": "3.7.5",
        "iso": "4.2.0",
        "istanbul": "0.3.22",
        "jsdom": "6.5.1",
        "mocha": "2.3.3",
        "react": "^0.14.0 || ^15.5.0",
        "react-dom": "^0.14.0 || ^15.5.0",
        "rimraf": "2.4.3",
        "sinon": "1.17.1"
